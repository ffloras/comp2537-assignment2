<%- include('template/header') %>
  <div class="container pb-5">
    <div class="row py-3">
      <h3>Hello <%= name %>!</h3>
    </div>
    <div class="row">
      <% for(let i=0; i < locals.imgLinks.length; i++) { %>
        <div class="col-lg d-flex justify-content-center">
          <img src="<%= locals.imgLinks[i] %>" class="object-fit-contain border rounded img-fluid">
        </div>
      <% } %>
    </div>
    <div class="row-sm-10 pt-3 pb-5">
      <a class="btn btn-primary" href="/logout" role="button">Sign Out</a>
    </div>
  </div>
  <%- include("template/footer") %>